{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
      "browser": true
    },
    "plugins": ["react"],
    "rules": {
      "linebreak-style": 0,
      "max-len": ["error", { "code": 120 }],
      "prefer-destructuring": ["error", {"object": false, "array": false}],
      "react/jsx-filename-extension": [
        2,
        {
          "extensions": [".js", ".jsx"]
        }
      ],
      "react/forbid-prop-types": [0],
      "react/require-default-props": [0],
      "global-require": [0]
    }
  }